class Pattern12 
{
	public static void main(String[] args) 
	{
		int n =20;
		int k=n;
		for (int i=1; i<=n; i++)
		{
			for (int j=1; j<=n-i+1; j++)
			{
				System.out.print(k);
				k--;
			}
			for (int j=1; j<=1; j++)
			{
				System.out.print("*");
			}
			k += n-i;
			System.out.println();
		}
	}
}
