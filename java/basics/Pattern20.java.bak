class Pattern20
{
	public static void main(String[] args) 
	{
		int n =9;
		for (int i=1; i<=n; i++)
		{
			if (i<=n/2+1)
			{
				for (int j=1; j<=i; j++)
				{
					if (j==1||j==i)
						System.out.print("*");
					else
						System.out.print(" ");
				}
			}
			else
			{
				for (int j=1; j<=n-i+1; j++)
				{
					if (j==1||j==n-i+1)
						System.out.print("*");
					else
						System.out.print(" ");
				}
				System.out.println();
			}
			
		}
	}
}

